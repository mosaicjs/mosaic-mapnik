Map {
  buffer-size: 256;
  background-color: blue; 
}

#basemap {
  text-name: "[NAME]";
  text-size: 12;
  text-face-name: "DejaVu Sans Bold", "Arial Italic";
  text-fill: black;
  text-halo-fill: #DFDBE3;
  text-halo-radius: 1;
  text-wrap-width : 20;
  
  polygon-fill: #f2eff9;
  polygon-opacity: 0.75;
  
  line-width: 0.1;
  line-color: #426;
}
